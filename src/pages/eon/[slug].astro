---

import Article from '@layouts/Article.astro';
import { entryToArticle, latestArticlesForWorld } from '../../utils/helpers';
import '../../styles/eon_theme.css';

// 1. Generate a new path for every collection entry
export async function getStaticPaths() {
    const worldArticles = await latestArticlesForWorld('eon');
    return worldArticles.map(entryToArticle);
}
const { article } = Astro.props;

---

<Article article={article} />